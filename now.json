{
  "version": 2,
  "builds": [
    {
      "src": "api/index-api.js",
      "use": "@now/node"
    },
    {
      "src": "post/index-post.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth(.*)",
      "dest": "/api/index-api.js"
    },
    {
      "src": "/api/user(.*)",
      "dest": "/api/index-api.js"
    },
    {
      "src": "/api/post(.*)",
      "dest": "/api/index-post.js"
    }
  ]
}